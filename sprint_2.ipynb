    "def average_points(team='Manchester United', season='2008/2009'):\n",
    "    \"\"\"Calculate the average points per game for a selected team and season.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    team : str, optional\n",
    "        The name of the team, by default 'Manchester United'.\n",
    "    season : str, optional\n",
    "        The name of the season, by default '2008/2009'.\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    float\n",
    "        Average points per game.\n",
    "    \"\"\"\n",
    "    return amount_of_points(team, season) / amount_of_games(team, season)"
   ]
