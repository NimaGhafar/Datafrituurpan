   "outputs": [],
   "source": [
    "def amount_of_points(team='Manchester United', season='2008/2009'):\n",
    "    \"\"\"Query the database to get the amount of points earned by a chosen team in a chosen season.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    team : str, optional\n",
    "        The name of the team, by default 'Manchester United'.\n",
    "    season : str, optional\n",
    "        The name of the season, by default '2008/2009'.\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    int\n",
    "        The amount of points earned.\n",
    "    \n",
    "    \"\"\"\n",
    "    return make_query(\n",
    "        f\"\"\"\n",
    "\n",
    "        SELECT SUM(CASE\n",
    "                   WHEN Home_team.team_long_name = '{team}' AND home_team_goal > away_team_goal THEN +3\n",
    "                   WHEN Home_team.team_long_name = '{team}' AND home_team_goal = away_team_goal THEN +1\n",
    "                   WHEN Away_team.team_long_name = '{team}' AND home_team_goal = away_team_goal THEN +1\n",
    "                   WHEN Away_team.team_long_name = '{team}' AND home_team_goal < away_team_goal THEN +3\n",
    "               END) AS amount_of_points\n",
    "        FROM Match\n",
    "        INNER JOIN Team AS Home_team\n",
    "            ON home_team_api_id = Home_team.team_api_id \n",
    "        INNER JOIN Team AS Away_team\n",
    "            ON away_team_api_id = Away_team.team_api_id\n",
    "        WHERE (Away_team.team_long_name LIKE '{team}' OR Home_team.team_long_name LIKE '{team}') AND season = '{season}'\n",
    "\n",
    "        \"\"\")['amount_of_points'][0]"
   ]
  },
