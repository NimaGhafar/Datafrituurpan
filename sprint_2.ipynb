   "source": [
    "player_columns = ['home_player_' + str(i) for i in range(1,12)] + ['away_player_' + str(i) for i in range(1, 12)]\n",
    "result = []\n",
    "\n",
    "for column in player_columns:\n",
    "    player_table = make_query(\n",
    "    f\"\"\"\n",
    "\n",
    "    SELECT {column},\n",
    "           player_name\n",
    "\n",
    "    FROM Match\n",
    "    INNER JOIN Player\n",
    "    ON Match.{column} = Player.player_api_id\n",
    "    WHERE home_team_api_id = '10260' OR away_team_api_id = '10260'\n",
    "\n",
    "    \"\"\")\n",
    "    result.append(player_table)\n",
    "\n",
    "player_data = pd.concat(result, axis=1)\n",
    "player_data"
   ]
  },
