    "def count_result(team='Manchester United', season='2008/2009', result='wins'):\n",
    "    \"\"\"Counts a how much a specific result has occured for a selected team and season.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    team : str, optional\n",
    "        The name of the team, by default 'Manchester United'.\n",
    "    season : str, optional\n",
    "        The name of the season, by default '2008/2009'.\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    int\n",
    "        The amount of this specific result.\n",
    "    \"\"\"\n",
    "    dataframe = home_away_wdl(team, season)\n",
    "    return dataframe[f'Home_{result}'][0] + dataframe[f'Away_{result}'][0]\n",
    "\n",
    "def goal_summary(team='Manchester United', season='2008/2009'):\n",
    "    \"\"\"Returns a goal summary for a selected team and season.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    team : str, optional\n",
    "        The name of the team, by default 'Manchester United'.\n",
    "    season : str, optional\n",
    "        The name of the season, by default '2008/2009'.\n",
    "    \n",
    "    Returns\n",
    "    -------\n",
    "    pandas.core.frame.DataFrame\n",
    "        Goal summary.\n",
    "    \"\"\"\n",
    "    return make_query(\n",
    "           f\"\"\"\n",
    "\n",
    "           SELECT CASE WHEN Home_team.team_long_name LIKE '{team}' THEN Home_team.team_long_name \n",
    "                      ELSE Away_team.team_long_name END AS Team_name,\n",
    "                  SUM(CASE WHEN Home_team.team_long_name = '{team}' THEN + home_team_goal\n",
    "                      ELSE +away_team_goal END) AS Goals_scored, \n",
    "                  SUM(CASE WHEN Home_team.team_long_name = '{team}' THEN + away_team_goal\n",
    "                      ELSE +home_team_goal END) AS Goals_against\n",
    "           \n",
    "           FROM Match\n",
    "           INNER JOIN Team AS Home_team\n",
    "               ON home_team_api_id = Home_team.team_api_id \n",
    "           INNER JOIN Team AS Away_team\n",
    "               ON away_team_api_id = Away_team.team_api_id\n",
    "           WHERE (Away_team.team_long_name LIKE '{team}' OR Home_team.team_long_name LIKE '{team}') AND season = '{season}'\n",
    "\n",
    "           \"\"\")\n",
    "\n",
    "def scoreboard(league='England Premier League', season='2008/2009'):\n",
    "    \"\"\"Returns a scoreboard for a given league and season.\n",
    "    \n",
    "    Parameters\n",
    "    ----------\n",
    "    league : str, optional\n",
    "        The name of the league, by default 'England Premier League'. \n",
    "        Make sure your league name has an exact match!\n",
    "    season : str, optional\n",
    "        The name of the season, by default '2008/2009'.\n",
    "        \n",
    "    Returns\n",
    "    -------\n",
    "    pandas.core.frame.DataFrame\n",
    "        The scorebord of a given league and season.\n",
    "    \"\"\"\n",
    "    clubs = list(make_query(\n",
    "    f\"\"\"\n",
    "\n",
    "    SELECT Distinct Home_team.team_long_name AS Club\n",
    "    FROM Match\n",
    "    INNER JOIN League League_name\n",
    "        ON Match.league_id = League_name.id\n",
    "    INNER JOIN Team AS Home_team\n",
    "        ON Match.home_team_api_id = Home_team.team_api_id \n",
    "    INNER JOIN Team AS Away_team\n",
    "        ON Match.away_team_api_id = Away_team.team_api_id\n",
    "    WHERE League_name.name LIKE '{league}' AND season = '{season}'\n",
    "\n",
    "    \"\"\")['Club'])\n",
    "\n",
    "    games = []\n",
    "    points = []\n",
    "    wins = []\n",
    "    draws = []\n",
    "    losses = []\n",
    "    goals_scored = []\n",
    "    goals_against = []\n",
    "    goal_difference = []\n",
    "    x = amount_of_games(clubs[0], season)\n",
    "\n",
    "    for club in clubs:\n",
    "        games.append(x)\n",
    "        points.append(amount_of_points(club, season))\n",
    "        wins.append(count_result(club, season, result='wins'))\n",
    "        draws.append(count_result(club, season, result='draws'))\n",
    "        losses.append(count_result(club, season, result='losses'))\n",
    "        y = goal_summary(club, season)\n",
    "        goals_scored.append(int(y['Goals_scored']))\n",
    "        goals_against.append(int(y['Goals_against']))\n",
    "        goal_difference.append(int(y['Goals_scored']) - int(y['Goals_against']))\n",
    "\n",
    "    return pd.DataFrame({'Clubs' : clubs,\n",
    "                         'Played games' : games,\n",
    "                         'Points' : points,\n",
    "                         'Wins' : wins,\n",
    "                         'Draws' : draws,\n",
    "                         'Losses' : losses,\n",
    "                         'Goals scored' : goals_scored,\n",
    "                         'Goals against' : goals_against,\n",
    "                         'Goal difference' : goal_difference\n",
    "                         }).sort_values(by='Points', ascending=False).set_index('Clubs')\n"
   ]
