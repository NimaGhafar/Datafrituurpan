   "source": [
    "make_query(\n",
    "\"\"\"\n",
    "\n",
    "SELECT team_long_name AS Team_naam,\n",
    "       team_api_id AS Team_id,\n",
    "       league_id AS Competitie_id,\n",
    "       name AS Competitie_naam\n",
    "FROM Team\n",
    "INNER JOIN Match\n",
    "    ON Team.team_api_id = Match.home_team_api_id\n",
    "INNER JOIN League AS League_name\n",
    "    ON Match.league_id = League_name.id\n",
    "WHERE team_long_name LIKE '%Manchester U%'\n",
    "\n",
    "\"\"\"\n",
    ").head(1)"
