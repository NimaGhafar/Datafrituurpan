   "source": [
    "match_data = make_query(\n",
    "\"\"\"\n",
    "\n",
    "SELECT League_name.id AS league_id,\n",
    "       League_name.name AS league_name,\n",
    "       season,\n",
    "       date,\n",
    "       Home_team.team_api_id AS home_team_id,\n",
    "       Home_team.team_long_name AS home_team,\n",
    "       Away_team.team_api_id AS away_team_id,\n",
    "       Away_team.team_long_name AS away_team,\n",
    "       home_team_goal,\n",
    "       away_team_goal,\n",
    "       CASE \n",
    "            WHEN home_team_api_id = '10260' AND home_team_goal > away_team_goal THEN 3\n",
    "            WHEN home_team_api_id = '10260' AND home_team_goal = away_team_goal THEN 1\n",
    "            WHEN home_team_api_id = '10260' AND home_team_goal < away_team_goal THEN 0\n",
    "            WHEN away_team_api_id = '10260' AND home_team_goal > away_team_goal THEN 0\n",
    "            WHEN away_team_api_id = '10260' AND home_team_goal = away_team_goal THEN 1\n",
    "            WHEN away_team_api_id = '10260' AND home_team_goal < away_team_goal THEN 3\n",
    "        END AS result_manchester_united\n",
    "        \n",
    "FROM Match\n",
    "INNER JOIN Team AS Home_team\n",
    "    ON Match.home_team_api_id = Home_team.team_api_id\n",
    "INNER JOIN Team AS Away_team\n",
    "    ON Match.away_team_api_id = Away_team.team_api_id\n",
    "INNER JOIN League AS League_name\n",
    "    ON Match.league_id = League_name.id\n",
    "\n",
    "WHERE home_team_api_id = '10260' OR away_team_api_id = '10260'\n",
    "\n",
    "\n",
    "\"\"\")\n",
    "\n",
    "match_data.sort_values(by='date')"
   ]
  },
